    "axios": "^1.2.1",
    "dayjs": "^1.11.6",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "firebase-admin": "^10.3.0",
    "firebase-functions": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "firebase-functions-test": "^0.2.0",
    "firebase-tools": "^11.16.1"
  },
  "private": true
}
